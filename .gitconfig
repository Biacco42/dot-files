[alias]
    st = status
    br = branch
    co = checkout
    cob = checkout -b
    adda = add -A
    cm = commit
    graph = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
    diffc = diff --cached
[user]
	email = biacco42@gmail.com
	name = Biacco42
[push]
	default = simple
[core]
	editor = vim -c \"set fenc=utf-8\"
	autocrlf = input
